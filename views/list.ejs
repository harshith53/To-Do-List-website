<%- include("header") -%>

    <div class="box" id="heading">
        <h1> <%= listTitle %></h1>
    </div>
    
    <div class="box">
    
       <% for(var i = 0; i <newlistitem.length; i++) { %>
        <div class="item">
            <input type="checkbox" id="">
            <p> <%= newlistitem[i] %></p>
        </div>
        <% } %>


        <form  class="item" action="/" method="post">
            <!-- <label for="newlistitem">Add a new list item</label> -->
            <input type="text" name="newitem" id="" placeholder="new items">
            <button type="submit" name="button" value=<%= listTitle %>>+</button>
        </form>


    </div>

<%- include("footer") -%>


  







